{
  "tasks": {
    "dev": "deno run --allow-net --allow-read --allow-env --watch main.ts",
    "migrate": "deno run --allow-net --allow-read --allow-env src/db/migrate.ts",
    "generate": "deno run --allow-net --allow-read --allow-write --allow-env npm:drizzle-kit generate:pg",
    "push": "deno run --allow-net --allow-read --allow-write --allow-env npm:drizzle-kit push:pg",
    "drop": "deno run --allow-net --allow-read --allow-write --allow-env npm:drizzle-kit drop",
    "studio": "deno run --allow-net --allow-read --allow-write --allow-env npm:drizzle-kit studio",
    "dev:docker": "docker compose -f docker-compose.dev.yml up --build -d",
    "dev:docker:down": "docker compose -f docker-compose.dev.yml down"
  },
  "nodeModulesDir": "auto",
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "@std/uuid": "jsr:@std/uuid@^1.0.4",
    "drizzle-kit": "npm:drizzle-kit@^0.30.1",
    "drizzle-orm": "npm:drizzle-orm@^0.38.3",
    "drizzle-zod": "npm:drizzle-zod@^0.6.1",
    "express": "npm:express@^4.21.2",
    "postgres": "npm:postgres@^3.4.5",
    "zennv": "npm:zennv@^0.1.1",
    "zod": "npm:zod@^3.24.1"
  }
}
