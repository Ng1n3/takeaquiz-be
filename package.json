{
  "name": "takeatest-be",
  "module": "main.ts",
  "type": "module",
  "devDependencies": {
    "@types/bun": "latest",
    "drizzle-kit": "^0.30.1",
    "eslint": "^9.17.0"
  },
  "scripts": {
    "dev": "bun --hot main.ts",
    "build": "bun build",
    "test": "bun run vitest",
    "lint": "bun run eslint src",
    "lint:fix": "bun run eslint src --fix",
    "db:push": "bun run drizzle-kit push",
    "db:studio": "bun run drizzle-kit studio",
    "db:generate": "bun run drizzle-kit generate",
    "db:seed": "bun src/dev/seed.ts",
    "db:migrate": "bun run drizzle-kit migrate",
    "dev:docker:up": "docker compose -f docker-compose.dev.yml up --build",
    "dev:docker:down": "docker compose -f docker-compose.dev.yml down",
    "docker:logs": "docker compose logs -f"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/lodash": "^4.17.13",
    "@types/nanoid": "^3.0.0",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.38.3",
    "drizzle-zod": "^0.6.1",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "nanoid": "^5.0.9",
    "postgres": "^3.4.5",
    "zennv": "^0.1.1",
    "zod": "^3.24.1"
  }
}
