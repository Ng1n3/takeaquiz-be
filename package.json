{
  "name": "takeatest-be",
  "module": "main.ts",
  "type": "module",
  "devDependencies": {
    "@types/bun": "latest",
    "drizzle-kit": "^0.30.1",
    "eslint": "^9.17.0"
  },
  "scripts": {
    "dev": "bun main.ts",
    "build": "bun build",
    "test": "bun run vitest",
    "lint": "bun run eslint src",
    "lint:fix": "bun run eslint src --fix",
    "db:push": "bun run drizzle-kit push",
    "db:studio": "bun run drizzle-kit studio",
    "db:generate": "bun run drizzle-kit generate",
    "db:seed": "bun src/dev/seed.ts",
    "db:migrate": "bun run drizzle-kit migrate",
    "dev:docker:up": "docker compose -f docker-compose.dev.yml up --build -d",
    "dev:docker:down": "docker compose -f docker-compose.dev.yml down"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.38.3",
    "drizzle-zod": "^0.6.1",
    "express": "^4.21.2",
    "postgres": "^3.4.5",
    "zennv": "^0.1.1",
    "zod": "^3.24.1"
  }
}
